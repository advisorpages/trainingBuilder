{
  "name": "@leadership-training/frontend",
  "version": "1.0.0",
  "private": true,
  "description": "React frontend for Leadership Training App",
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@leadership-training/shared": "*",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.89.0",
    "@types/recharts": "^1.8.29",
    "axios": "^1.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build",
    "build:analyze": "npm run build && open dist/stats.html",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.{ts,tsx}",
    "test:components": "vitest run --reporter=verbose src/components/**/*.test.{ts,tsx}",
    "test:watch": "vitest --watch",
    "test:coverage": "echo 'Coverage temporarily disabled due to performance issues. Use npm test instead.'",
    "test:coverage:services": "vitest --coverage src/services",
    "test:coverage:full": "vitest --coverage --config vitest.full.config.ts",
    "test:coverage:simple": "vitest run --reporter=verbose && echo 'Tests completed successfully. Coverage analysis disabled for performance.'",
    "test:ci": "vitest --run --coverage",
    "test:ui": "vitest --ui"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
